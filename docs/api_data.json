ents/:id/open",    "title": "Request a toke for opening file.",    "name": "OpenDocument",    "group": "Documents",    "version": "0.0.1",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "ObjectId",            "optional": false,            "field": "id",            "description": "<p>Document unique Object Id.</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "token",            "description": "<p>access token that flip server to open document.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n  {\n     \"_id\": \"58b1ab53b65b10af1123409e\",\n   }",          "type": "json"        }      ]    },    "filename": "app/routes/api/documents.js",    "groupTitle": "Documents",    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "DocumentNotFound",            "description": "<p>The id of the Document was not found.</p>"          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 404 Not Found\n{\n   \"error\": true,\n   \"message\": \"DocumentNotFound\"\n}",          "type": "json"        }      ]    }  },  {    "type": "put",    "url": "/documents/:id",    "title": "Update a Document",    "name": "UpdateDocument",    "group": "Documents",    "version": "0.0.1",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "id",            "description": "<p>The Document unique Object Id.</p>"          } 